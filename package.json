{
  "name": "beeb_backend",
  "version": "0.0.1",
  "description": "backend of Blues en el Balcón website",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf dist && webpack --mode production --config webpack.config.js",
    "build-dev": "rm -rf dist && webpack --mode development --config webpack.config.js",
    "start": "node server.js",
    "lint": "eslint --ext js src",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test",
      "post-merge": "npm install",
      "post-rewrite": "npm install"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blues-en-el-Balcon/Backend.git"
  },
  "author": "Alejo López, Juan Pablo Hernandez",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Blues-en-el-Balcon/Backend/issues"
  },
  "homepage": "https://github.com/Blues-en-el-Balcon/Backend#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
